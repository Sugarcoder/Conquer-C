// "Continue" forces a loop to continue early.


/* This program loops through 10 numbers and prints a message that varies whether the program is odd or even. */




#include <stdio.h>

main()

{
    int i;

    // Loops through number 1 through 10

    for (i = 1; i <= 10; i++)
    {
        if ((i%2) == 1)     // Odd numbers have a remainder of 1
        {
            printf("I'm rather odd...\n");
            
            // Jump to the next iteration of the loop

            continue;
        }

        printf("Even up!\n");
    }


    return 0;

}
