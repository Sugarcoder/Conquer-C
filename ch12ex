#include <stdio.h>

main()

{
    char name[25];
    printf("What is your last name? ");
    printf("Please capitalize the first letter.\n");
    scanf(" %s", name);

    if ((name[0] >= 'P') && (name <= 'S'))
    {
        printf("You must go to room 201");
        printf("for your tickets.\n");
    }

    else
        {
        printf("You can get your tickets here.\n");
        }

    return 0;

}
